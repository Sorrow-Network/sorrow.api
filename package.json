{
  "name": "sorrow.api",
  "version": "1.2.1",
  "description": "The best api for anime images, jokes, animals, facts, roasts, and more.",
  "main": "src/index.ts",
  "scripts": {
    "build": "npx tsup src/index.ts --minify",
    "test": "nyc ava",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sorrow-Network/sorrow.api.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "sorrowapi",
    "dog",
    "cat",
    "bird",
    "panda",
    "koala",
    "animal api",
    "random animal image api"
  ],
  "author": "isorrowdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sorrow-Network/sorrow.api/issues"
  },
  "homepage": "https://github.com/Sorrow-Network/sorrow.api#readme",
  "dependencies": {
    "node-superfetch": "^0.1.11"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "import-to-array": "^1.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
